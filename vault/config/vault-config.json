{
    "listener": [
        {
            "tcp": {
                "address": "0.0.0.0:8200",
                "tls_disable": 0,
                "tls_cert_file": "${SSL_CERT_PATH}${SSL_CERT_FILE}",
                "tls_key_file": "${SSL_CERT_KEY_PATH}${SSL_CERT_KEY_FILE}"
            }
        }
    ],
    "storage": {
        "file": {
            "path": "/vault/data"
        }
    },
    "ui": true,
    "api_addr": "https://0.0.0.0:8200"
}